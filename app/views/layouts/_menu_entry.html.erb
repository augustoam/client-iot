<li>
  <%= link_to home_console_path do %>
  <i class="material-icons">dashboard</i>
  <span class="grey-text text-darken-4">In√≠cio</span>
<% end %>
</li>
<% grupos_por_usuario.each do |grupo| %>
<li class="no-padding">
  <ul class="collapsible collapsible-accordion">
    <li>
      <a class="collapsible-header white-text "><%= grupo.grupo.nome %>
        <i class="material-icons"></i>
      </a>
      <% grupo.grupo.ambientes.each do |ambiente| %>
        <% if ambiente.visivel %>
          <div class="collapsible-body grey darken-3">
            <ul>
              <li><%= link_to ambiente.nome, grupo_ambiente_componentes_ambiente_path(grupo, ambiente), class: "white-text" %></li>
            </ul>
          </div>
        <% end %>
      <% end %>
    </li>
  </ul>
</li>
<% end %>
